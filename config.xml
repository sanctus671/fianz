<?xml version='1.0' encoding='utf-8'?>
<widget id="app.fianz.islamnz" version="0.0.3" xmlns="http://www.w3.org/ns/widgets" xmlns:android="http://schemas.android.com/apk/res/android" xmlns:cdv="http://cordova.apache.org/ns/1.0" xmlns:gap="http://phonegap.com/ns/1.0">
    <edit-config file="app/src/main/AndroidManifest.xml" mode="merge" target="/manifest/application/activity[@android:label='@string/activity_name']">
        <activity android:theme="@android:style/Theme.Material.Light.NoActionBar" />
    </edit-config>
    <name>Islam In New Zealand</name>
    <description>Muslim Prayer App</description>
    <author email="info@wentity.com" href="http://www.wentity.com/">wEntity</author>
    <content src="index.html" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="phonegap-version" value="cli-7.1.0" />
    <preference name="pgb-builder-version" value="2" />
    <preference name="ScrollEnabled" value="false" />
    <preference name="android-minSdkVersion" value="19" />
    <preference name="android-targetSdkVersion" value="27" />
    <preference name="BackupWebStorage" value="none" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="AutoHideSplashScreen" value="false" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashScreen" value="screen" />
    <preference name="ShowSplashScreenSpinner" value="false" />
    <preference name="SplashScreenBackgroundColor" value="#d44735" />
    <preference name="WKSuspendInBackground" value="false" />
    <platform name="android">
        <hook src="hooks/patch-android-studio-check.js" type="before_plugin_install" />
        <hook src="hooks/patch-android-studio-check.js" type="before_plugin_add" />
        <hook src="hooks/patch-android-studio-check.js" type="before_build" />
        <hook src="hooks/patch-android-studio-check.js" type="before_run" />
        <hook src="hooks/patch-android-studio-check.js" type="before_plugin_rm" />
        <preference name="StatusBarStyle" value="lightcontent" />
        <preference name="StatusBarBackgroundColor" value="#035003" />
        <allow-intent href="market:*" />
        <preference name="loadUrlTimeoutValue" value="700000" />
        <preference name="android-build-tool" value="gradle" />
        <icon density="ldpi" src="resources/android/icon/ldpi.png" />
        <icon density="mdpi" src="resources/android/icon/mdpi.png" />
        <icon density="hdpi" src="resources/android/icon/hdpi.png" />
        <icon density="xhdpi" src="resources/android/icon/xhdpi.png" />
        <icon density="xxhdpi" src="resources/android/icon/xxhdpi.png" />
        <icon density="xxxhdpi" src="resources/android/icon/xxxhdpi.png" />
        <resource-file src="resources/android/icon/mdpi_small_icon.png" target="app/src/main/res/drawable-mdpi-v4/small_icon.png" />
        <resource-file src="resources/android/icon/hdpi_small_icon.png" target="app/src/main/res/drawable-hdpi-v4/small_icon.png" />
        <resource-file src="resources/android/icon/xhdpi_small_icon.png" target="app/src/main/res/drawable-xhdpi-v4/small_icon.png" />
        <resource-file src="resources/android/icon/xxhdpi_small_icon.png" target="app/src/main/res/drawable-xxhdpi-v4/small_icon.png" />
        <resource-file src="resources/android/icon/xxxhdpi_small_icon.png" target="app/src/main/res/drawable-xxxhdpi-v4/small_icon.png" />
    </platform>
    <splash density="port-ldpi" platform="android" src="resources/android/splash/splash-port-ldpi.png" />
    <splash density="port-mdpi" platform="android" src="resources/android/splash/splash-port-mdpi.png" />
    <splash density="port-hdpi" platform="android" src="resources/android/splash/splash-port-hdpi.png" />
    <splash density="port-xhdpi" platform="android" src="resources/android/splash/splash-port-xhdpi.png" />
    <splash density="port-xxhdpi" platform="android" src="resources/android/splash/splash-port-xxhdpi.png" />
    <splash density="port-xxxhdpi" platform="android" src="resources/android/splash/splash-port-xxxhdpi.png" />
    <splash density="land-ldpi" platform="android" src="resources/android/splash/splash-land-ldpi.png" />
    <splash density="land-mdpi" platform="android" src="resources/android/splash/splash-land-mdpi.png" />
    <splash density="land-hdpi" platform="android" src="resources/android/splash/splash-land-hdpi.png" />
    <splash density="land-xhdpi" platform="android" src="resources/android/splash/splash-land-xhdpi.png" />
    <splash density="land-xxhdpi" platform="android" src="resources/android/splash/splash-land-xxhdpi.png" />
    <splash density="land-xxxhdpi" platform="android" src="resources/android/splash/splash-land-xxxhdpi.png" />
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
        <preference name="ios-statusbarstyle" value="black-opaque" />
		
		<edit-config target="NSLocationWhenInUseUsageDescription" file="*-Info.plist" mode="merge">
			<string>We use your location to show you the most relevant channels in your area</string>
		</edit-config>		
		
        <icon height="1024" src="resources/ios/icon/icon-1024.png" width="1024" />
        <icon height="29" src="resources/ios/icon/icon-small.png" width="29" />
        <icon height="58" src="resources/ios/icon/icon-small@2x.png" width="58" />
        <icon height="87" src="resources/ios/icon/icon-small@3x.png" width="87" />
        <icon height="40" src="resources/ios/icon/icon-small-40.png" width="40" />
        <icon height="80" src="resources/ios/icon/icon-small-40@2x.png" width="80" />
        <icon height="120" src="resources/ios/icon/icon-small-40@3x.png" width="120" />
        <icon height="50" src="resources/ios/icon/icon-small-50.png" width="50" />
        <icon height="100" src="resources/ios/icon/icon-small-50@2x.png" width="100" />
        <icon height="57" src="resources/ios/icon/icon.png" width="57" />
        <icon height="114" src="resources/ios/icon/icon@2x.png" width="114" />
        <icon height="60" src="resources/ios/icon/icon-60.png" width="60" />
        <icon height="120" src="resources/ios/icon/icon-60@2x.png" width="120" />
        <icon height="180" src="resources/ios/icon/icon-60@3x.png" width="180" />
        <icon height="72" src="resources/ios/icon/icon-72.png" width="72" />
        <icon height="144" src="resources/ios/icon/icon-72@2x.png" width="144" />
        <icon height="76" src="resources/ios/icon/icon-76.png" width="76" />
        <icon height="152" src="resources/ios/icon/icon-76@2x.png" width="152" />
        <icon height="167" src="resources/ios/icon/icon-167.png" width="167" />
        <icon height="167" src="resources/ios/icon/icon-83.5@2x.png" width="167" />
    </platform>
    <splash height="480" platform="ios" src="resources/ios/splash/Default~iphone.png" width="320" />
    <splash height="960" platform="ios" src="resources/ios/splash/Default@2x~iphone.png" width="640" />
    <splash height="1024" platform="ios" src="resources/ios/splash/Default-Portrait~ipad.png" width="768" />
    <splash height="2048" platform="ios" src="resources/ios/splash/Default-Portrait@2x~ipad.png" width="1536" />
    <splash height="1136" platform="ios" src="resources/ios/splash/Default-568h@2x~iphone.png" width="640" />
    <splash height="1334" platform="ios" src="resources/ios/splash/Default-667h.png" width="750" />
    <splash height="2208" platform="ios" src="resources/ios/splash/Default-736h.png" width="1242" />
    <splash height="768" platform="ios" src="resources/ios/splash/Default-Landscape~ipad.png" width="1024" />
    <splash height="1536" platform="ios" src="resources/ios/splash/Default-Landscape@2x~ipad.png" width="2048" />
    <splash platform="ios" src="resources/ios/splash/Default@2x~iphone~anyany.png" />
    <splash platform="ios" src="resources/ios/splash/Default@2x~iphone~comany.png" />
    <splash platform="ios" src="resources/ios/splash/Default@3x~iphone~anyany.png" />
    <splash platform="ios" src="resources/ios/splash/Default@3x~iphone~comany.png" />
    <splash platform="ios" src="resources/ios/splash/Default@2x~ipad~anyany.png" />
    <splash platform="ios" src="resources/ios/splash/Default@2x~ipad~comany.png" />
    <plugin name="cordova-plugin-whitelist" spec="^1.3.1" />
    <plugin name="cordova-plugin-device" spec="^1.1.4" />
    <gap:plugin spec="https://github.com/pdyraga/cordova-plugin-splashscreen" />
    <plugin name="cordova-plugin-ionic-webview" spec="^2.4.1" />
    <plugin name="ionic-plugin-keyboard" spec="^2.2.1" />
    <plugin name="cordova-plugin-statusbar" spec="2.4.2" />
	
    <plugin name="cordova-plugin-nativegeocoder" spec="3.1.2">
		<variable name="LOCATION_WHEN_IN_USE_DESCRIPTION" value="We use your location to show you the best channels to subscribe to" />
	</plugin>
    <plugin name="cordova-plugin-geolocation" spec="4.0.1">
        <variable name="GEOLOCATION_USAGE_DESCRIPTION" value="We use your location to show you the best channels to subscribe to" />
    </plugin>
    <plugin name="cordova-plugin-inappbrowser" spec="^1.7.2" />
    <plugin name="cordova-plugin-network-information" spec="2.0.1" />
    <plugin name="onesignal-cordova-plugin" spec="2.4.5" />
    <plugin name="cordova.plugins.DozeOptimize" spec="https://github.com/thomas550i/cordova-plugin-doze-Optimize" />
    <plugin name="cordova-plugin-badge" spec="^0.8.7" />
    <plugin name="cordova-plugin-local-notification" spec="https://github.com/sanctus671/cordova-plugin-local-notifications" />
    <plugin name="cordova-plugin-fastrde-mqtt" spec="https://github.com/sanctus671/cordova-plugin-fastrde-mqtt#master" />
    <engine name="android" spec="7.1.1" />
</widget>
